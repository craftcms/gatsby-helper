{
  "name": "craftcms/gatsby-helper",
  "description": "Plugin for enabling support for the Gatsby Craft CMS source plugin.",
  "type": "craft-plugin",
  "version": "1.1.2",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "gatsby"
  ],
  "support": {
    "docs": "https://github.com/craftcms/gatsby-helper",
    "issues": "https://github.com/craftcms/gatsby-helper/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel & Tonic",
      "homepage": "https://pixelandtonic.com/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "^3.5.11"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "craft\\gatsbyhelper\\": "src/"
    }
  },
  "extra": {
    "name": "Gatsby Helper",
    "handle": "gatsby-helper",
    "developerUrl": "https://craftcms.com/"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "platform": {
      "php": "7.2.5"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
